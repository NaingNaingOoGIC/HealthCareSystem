<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="healthcaresystem" match-name="admin"/>
  <table-filter match-catalog="healthcaresystem" match-name="department"/>
  <table-filter match-catalog="healthcaresystem" match-name="doctor"/>
  <table-filter match-catalog="healthcaresystem" match-name="patient"/>
  <table-filter match-catalog="healthcaresystem" match-name="patient_progress"/>
  <table-filter match-catalog="healthcaresystem" match-name="progress_detail"/>
</hibernate-reverse-engineering>